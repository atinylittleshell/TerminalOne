    "@types/node-fetch": "^2.6.2",
    "babel-loader": "^8.2.5",
    "shx": "^0.3.4",
    "typescript": "^5.0.4",
    "webpack-cli": "^4.9.2"
  },
  "dependencies": {
    "@terminalone/terminal": "*",
    "dayjs": "^1.11.7",
    "electron": "^24.1.2",
